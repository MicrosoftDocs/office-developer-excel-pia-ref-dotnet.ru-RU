    <Member MemberName="AutoAttach">
      <MemberSignature Language="C#" Value="public Microsoft.Office.Core.MsoTriState AutoAttach { get; set; }" />
      <MemberSignature Language="ILAsm" Value=".property instance valuetype Microsoft.Office.Core.MsoTriState AutoAttach" />
