    <Member MemberName="ShowDetail">
      <MemberSignature Language="C#" Value="public bool ShowDetail { get; set; }" />
      <MemberSignature Language="ILAsm" Value=".property instance bool ShowDetail" />
      <MemberSignature Language="DocId" Value="P:Microsoft.Office.Interop.Excel.PivotItem.ShowDetail" />
      <MemberSignature Language="VB.NET" Value="Public Property ShowDetail As Boolean" />
